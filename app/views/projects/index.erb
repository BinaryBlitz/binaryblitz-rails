<%= provide(:title, t('.title')) %>

<% cache do %>
  <!-- Works -->
  <section class="works-wrap">
    <div class="container">
      <div class="works">
        <div class="works-nav">
          <ul>
            <li><%= link_to t('.apps'), '#', class: 'active' %></li>
            <li><%= link_to t('.web'), '#' %></li>
          </ul>
        </div>

        <div class="row works-items">
          <div class="col-sm-6 works-item works-item_light">
            <div class="works-bg" style="background-image: url(/images/projects/fitmost/background.png)"></div>
            <%= link_to '', fitmost_path, class: 'works-hover' %>
            <div class="works-logo">
              <img alt="" src="/images/projects/fitmost/logo-round.png">
            </div>
            <span class="works-type"><%= t('projects.types.app') %></span>
            <p class="works-name">
              <%= t('projects.fitmost.head') %>
            </p>
          </div>
